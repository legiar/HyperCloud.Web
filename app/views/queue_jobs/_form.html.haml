= form_for(@queue_job) do |f|
  - if @queue_job.errors.any?
    #errorExplanation
      %h2= "#{pluralize(@queue_job.errors.count, 'error')} prohibited this user from being saved:"
      %ul
      - @queue_job.errors.full_messages.each do |msg|
        %li= msg
  
  .field
    = f.label :job
    %br
    = f.text_field :job
  .field
    = f.label :status
    %br
    = f.text_field :status
  .field
    = f.label :completed_at
    %br
    = f.text_field :completed_at

  .actions
    = f.submit
