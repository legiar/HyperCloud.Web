- @title = "Users"

= link_to "New user", new_user_path

%table.table
  %thead
    %tr
      = table_columns %w(email)
  %tbody
    - @users.each do |user|
      %tr
        %td= user.id
        %td= user.email
        %td.actions
          = link_to "Show", user
          = link_to "Edit", edit_user_path(user)
